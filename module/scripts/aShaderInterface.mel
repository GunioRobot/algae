/*
convert texture
*/

global proc anm_sdr_makeTexture(string $textfield)
{
	string $file = `textFieldButtonGrp -q -text $textfield`;
	if($file != "") {
		string $buffer[];
		$numTokens = `tokenize $file "." $buffer`;
		int $i;
		string $tdl = "";
		for($i=0; $i<$numTokens - 1; $i++) {
			$tdl = $tdl + $buffer[$i] + ".";
		}
		$tdl = $tdl + "tdl";
		system("tdlmake -mode black "+$file+" "+$tdl);
		
		textFieldButtonGrp -e -text $tdl $textfield;
		 
		string $attr = `textFieldButtonGrp -q -ann $textfield`;
		
		setAttr -type "string" $attr $tdl;
	}
}

