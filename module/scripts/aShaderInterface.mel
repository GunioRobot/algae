						button -l "Clear" -c ("anm_clearStringAttrByScrollField(\""+$sclcur+"\")");
					setParent..;
				}
				else if($dtl=="pointcloud") {

					string $txctr = `textFieldButtonGrp -cw 1 50 -cw 2 200 -cw 3 40
								-l "Value" 
								-bl "<...>"
								-text $scur 
								-ann ($name+".value")`;
					textFieldButtonGrp -e -cc ("anm_connectTextFieldButtonToStringAttr(\""+$txctr+"\")") $txctr;
		// pick up a file
					textFieldButtonGrp -e -bc ("anm_pickupFileAsStringAttr(\""+$txctr+"\")") $txctr;
						
					anm_appendPointCloudEpr($txctr);
						
					//scrollField -e -cc ("anm_stringAttrByScrollField(\""+$sclcur+"\")") $sclcur;
					
				}
				else 
					textField -text $scur;
			}
		}
        		
		setParent..;
	setParent..;
